extends ../base/_template_html.pug

block page-id
  - const $pageId = 'partners';

block page-main
  section(class=setModifiers('page-main__nest', ['partners-first']))
    +partners()(heading='Наши партнеры')
  section(class=setModifiers('page-main__nest', ['acknowledgements']))
    include ../components/capital/acknowledgements/_acknowledgements.pug
  section(class=setModifiers('page-main__nest', ['partners-last']))
    +partners()(heading='Нам доверяют')

block page-scripts-distinct
  script(src='js/acknowledgements-slider.js')
  script(src='js/modal.js')
